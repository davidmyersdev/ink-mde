{
  "name": "@writewithocto/vue-ink",
  "description": "The flexible TypeScript Markdown editor that powers https://octo.app - packaged as a Vue component.",
  "license": "MIT",
  "version": "v0.7.0",
  "main": "./dist/ink.cjs",
  "module": "./dist/ink.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/ink.js",
      "require": "./dist/ink.cjs"
    }
  },
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "build:demo": "vue-tsc --noEmit && vite --config demo.config.ts build --base /vue-ink",
    "dev": "vite"
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "@vitejs/plugin-vue": "^3.0.0",
    "@writewithocto/ink": "workspace:*",
    "typescript": "^4.7.4",
    "vite": "^3.0.2",
    "vue": "^3.2.37",
    "vue-tsc": "^0.38.9"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "codemirror",
    "component",
    "easymde",
    "javascript",
    "js",
    "markdown",
    "mde",
    "octo",
    "sfc",
    "simplemde",
    "ts",
    "typescript",
    "vue",
    "vue3"
  ]
}
